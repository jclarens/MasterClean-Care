<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.TA.MVP.appmobilemember">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.TA.MVP.appmobilemember.View.Activity.ParentActivity"></activity>
        <activity android:name="com.TA.MVP.appmobilemember.View.Activity.AuthActivity"></activity>
        <activity android:name="com.TA.MVP.appmobilemember.View.Activity.ProfileActivity"></activity>
        <activity android:name="com.TA.MVP.appmobilemember.View.Activity.EditProfileActivity"></activity>
        <activity
            android:name="com.TA.MVP.appmobilemember.View.Activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>